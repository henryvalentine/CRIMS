@model Crims.Data.Models.BaseData
@{
    Layout = null;
}
<div class="box box-info">
    <div class="box-header with-border">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Custom Data Enrollment</h4>
    </div>
   
    <div class="form-horizontal">
        <div class="box-body">
            <input type="hidden" id="tableId" />
            <div class="form-group">
                <label class="control-label col-sm-3">
                    Enrollment Id
                </label>
                <div class="col-sm-8">
                    <h4 id="EnrollmentId"></h4>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-3">
                    Project Primary Code
                </label>
                <div class="col-sm-8">
                    <h4 id="ProjectPrimaryCode"></h4>
                </div>
            </div>
        <div class="form-group">
            <label class="control-label col-sm-3">
                Names
            </label>
            <div class="col-sm-8">
                <h4 id="Name"></h4>
            </div>
        </div>
        <hr/>
        <div class="col-md-12 customContainer">
            
         </div>
         <div class="box-footer">
                <div class="col-md-12">
                    <div class="col-md-4">
                    </div>
                    <div class="col-md-4">
                        <button type="button" class="btn btn-default" onclick="saveThis()">Save</button>
                    </div>
              </div>
          </div>
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
            <div class="col-md-12">
                <div class="col-md-8">
                </div>
                <div class="col-md-4">
                    <button type="button" class="btn btn-default" onclick="cancel()">Cancel</button>
                </div>
            </div>
        </div>
        <!-- /.box-footer -->
    </div>
</div>