@model Crims.Data.Models.CustomListData
@{
    Layout = null;
}
<div class="box box-info">
    <div class="box-header with-border">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Create Custom Group</h4>
    </div>
    <!-- /.box-header -->
    <!-- form start -->
    <div class="form-horizontal">
        <div class="box-body">
            <input type="hidden" id="tableId" />
            <div class="form-group">
                <label class="control-label col-sm-3">
                    Custom Group Name
                </label>
                <div class="col-sm-8">
                    @Html.EditorFor(model => model.ListDataName, new {htmlAttributes = new {@class = "form-control"}})
                    @Html.ValidationMessageFor(model => model.ListDataName, "", new {@class = "text-danger"})
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-3">
                    Custom List
                </label>
                <div class="col-sm-8">
                    <select class="form-control" id="CustomListId">
                        <option value="0">-- Select option --</option>
                    </select>
                </div>
            </div>
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
            <button type="button" class="btn btn-info pull-right" onclick="processCustomListData()">Save</button>
        </div>
        <!-- /.box-footer -->
    </div>
</div>